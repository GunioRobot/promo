<h1>Listing affiliate_networks</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @affiliate_networks.each do |affiliate_network| %>
  <tr>
    <td><%= affiliate_network.name %></td>
    <td><%= link_to 'Show', affiliate_network %></td>
    <td><%= link_to 'Edit', edit_affiliate_network_path(affiliate_network) %></td>
    <td><%= link_to 'Destroy', affiliate_network, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

